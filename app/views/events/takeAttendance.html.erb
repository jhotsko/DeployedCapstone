<p id="notice"><%= notice %></p>

<h2>Event Attendance</h2>

<table>
  <thead>
    <tr>
      <th>Cadetid</th>
      <th>Lastname</th>
      <th>Firstname</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cadets.each do |cadet| %>
      <tr>
        <td><%= cadet.cadetID %></td>
        <td><%= cadet.lastName %></td>
        <td><%= cadet.firstName %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'Events Display', events_path %> 


